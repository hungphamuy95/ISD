<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="~/Images/logo.ico" />
    <link href="~/Content/Custom.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.validate-vsdoc.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/respond.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.15.0/jquery.validate.min.js"></script>
    <script src='https://www.google.com/recaptcha/api.js'></script>
    @*<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    @*<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>*@
    <script src="~/Content/bootstrap.min.js"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>*@
    <script src="~/Content/jquery.min.js"></script>
    <script src="/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    @*<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>*@
    <script src="~/Content/angular.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.1/angular-sanitize.js"></script>*@
    <script src="~/Content/angular-sanitize.js"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.1/angular-animate.min.js"></script>*@
    <script src="~/Content/angular-animate.min.js"></script>
    <script src="~/assets/plugins/ckeditor/ckeditor.js"></script>
    <script src="~/assets/plugins/ckfinder/ckfinder.js"></script>
    <script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '686794621468795',
      xfbml      : true,
      version    : 'v2.7'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
    </script>
</head>
<body>
    <div class="container">
        <div class="header">
            <div class="row">
                <div class="col-lg-6">
                    <img src="~/Images/logo.png" alt="logo" style="height:70px; width:70px;" />
                </div>
                <div class="col-lg-2"></div>
                <div class="col-lg-4">

                    <span style="color:cornflowerblue;">HOTLINE:</span>&nbsp;<span style="color:red;">03203 747 630</span>&nbsp;
                    <span class="glyphicon glyphicon-envelope"></span>Webmail
                </div>
            </div>
        </div>
        <br />
        <div id="custom-bootstrap-menu" class="navbar navbar-default " role="navigation">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-menubuilder">
                        <span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                    </button>
                </div>
                <div class="collapse navbar-collapse navbar-menubuilder">
                    <ul class="nav navbar-nav navbar-left">
                        <li id="home">
                            <a href="@Url.Action("Index","Home")">TRANG CHỦ</a>
                        </li>
                        <li id="school">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">TRƯỜNG HỌC</a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="3"})">LỊCH SỬ</a></li>
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="4"})">THÀNH TÍCH</a></li>
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="5"})">BAN GIÁM HIỆU</a></li>
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="6"})">HOẠT ĐỘNG</a></li>
                            </ul>
                        </li>
                        <li id="edu">
                            <a href="@Url.Action("EventNews","Home", new { @groupnewsid="7"})">GIÁO DỤC</a>
                        </li>
                        <li id="material">
                            <a href="@Url.Action("Material","Home", new { @groupnewsid="8"})">CƠ SỞ VẬT CHẤT</a>
                        </li>
                        <li id="staff">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">ĐỘI NGŨ GIÁO VIÊN</a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="10"})">TỔ KHOA HỌC TỰ NHIÊN</a></li>
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="11"})">TỔ KHOA HỌC XÃ HỘI</a></li>
                                <li><a href="@Url.Action("EventNews","Home", new { @groupnewsid="12"})">TỔ VĂN PHÒNG</a></li>
                            </ul>
                        </li>
                        <li id="student">
                            <a href="@Url.Action("EventNews","Home", new { @groupnewsid="13"})">HỌC SINH TIÊU BIỂU</a>
                        </li>
                        <li id="news">
                            <a href="@Url.Action("NewsList","Home")">TIN TỨC</a>
                        </li>
                        <li id="contact">
                            <a href="@Url.Action("Contact","Home", new { @groupnewsid="15"})">LIÊN HỆ</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                @if (TempData["AlertMessage"] != null)
                {
                    <div class="alert @TempData["AlertType"]" id="alertbox">
                        @TempData["AlertMessage"]
                    </div>
                }
                @RenderBody()
                @RenderPage("CkSource.cshtml")
            </div>
        </div>
        <div class="row">
            <div class="nav navbar-default" style="background:#1c3e61;">
                <br />
                <div class="col-lg-6"><span style="color:white;">Bản quyền &copy;@DateTime.Now.Year thuộc về Trường trung học cơ sở Thị trấn Tứ Kỳ<br />Thị trấn Tứ Kỳ, huyện Tứ Kỳ, tỉnh Hải Dương</span><br />&nbsp;</div>
                <div class="col-lg-2"></div>
                <div class="col-lg-4"><span style="color:white;">Phát triển bởi công ty Crystal</span><br />&nbsp;</div>
            </div>
        </div>
    </div>
    <!-- Go to www.addthis.com/dashboard to customize your tools -->
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-57e2923a0b0bf39c"></script>
</body>
</html>
