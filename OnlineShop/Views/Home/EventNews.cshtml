@using TK.Business.Data
@{
    TblGroupNew o = new TblGroupNew();
    o = (TblGroupNew)ViewBag.GroupNewsName;
    ViewBag.Title = o.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="panel panel-primary">
    
        @{
            TblNewsDraff obj = new TblNewsDraff();
            obj = (TblNewsDraff)ViewBag.School;
        }
    <div class="panel-heading">
        <h5>@obj.Title</h5>
    </div>
    <div class="panel-body">
        @Html.Raw(System.Web.HttpUtility.HtmlDecode(obj.Content))
    </div>
    
</div>
<br />

